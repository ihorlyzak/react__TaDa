{
	"extends": ["react-app", "react-app/jest", "prettier"],
	"rules": {
		"no-unused-vars": "warn",
		"prefer-const": "warn",
		"no-console": "warn",
		"quotes": ["error", "single"],
		"jsx-quotes": ["error", "prefer-single"],
		"comma-dangle": ["error", "always-multiline"],
		"semi": ["warn", "always"]
	}
}
